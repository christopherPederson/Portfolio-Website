    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "globals": {
            "define": "readonly",
            "module": "readonly",
            "window": "writable",
            "global": "writable",
            "self": "writable"
        },
        "rules": {
            "no-undef": "warn",
            "no-unused-expressions": "off",
            "no-restricted-globals": [
                "error",
                {
                    "name": "self",
                    "message": "Use window or global instead."
                }
            ]
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "tailwindcss": "^3.4.1"
    }
}
