        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-github-calendar": "^4.1.3",
        "react-particles-js": "^3.6.0",
        "react-router-dom": "^6.22.2",
        "react-scripts": "5.0.1",
